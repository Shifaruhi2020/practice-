class Solution {
    public int findGCD(int[] nums) {
        int n;
        n=nums.length-1;
        Arrays.sort(nums);
   int min=nums[0];      
    int max=nums[n];
        return isgcd(min,max);
    }
    
    int isgcd(int a,int b){
        if(b>a){
            return isgcd(b,a);
        }
        if(b==0){
            return a;
        }
            return isgcd(b,a%b);
    }
}
