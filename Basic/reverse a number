public class Main{
    public static void main (String[] args) {
        int n;
        n=123;
        //reversing a Number
        int temp,rem;
        temp=n;
        int rev=0;
        while(temp!=0){
            rem=temp%10;
        rev=rev*10+rem;
        temp=temp/10;
        }
        System.out.println(rev);
    }
}
