public class Printing 
{
    public static void see(int n)
    {
    for (int i=0;i<=n;i++)
    {
        for(int j=0;j<=i;j++)
     {
        System.out.print("*");
     }
        System.out.println();
    }
    }
    
    
    
    /////// the main methid is starts here 
    public static void main(String[] args)
    {
        int n=5;
        see(n);
    }
}
