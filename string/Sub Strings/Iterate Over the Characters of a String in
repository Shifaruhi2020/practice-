import java.io.*;
import java.util.*;
public class Main
{
   static void iterate(String s)
   {
       for(int i=0 ; i<s.length() ; i++)
       {
           System.out.println(s.charAt(i)+" ");
       }
       
   }
   
   public static void main (String[] args) {
       //String s="Welcome";
       Scanner sc=new Scanner(System.in);
 
      String s=sc.nextLine();
      System.out.println();
      
      iterate(s);

   }
}
