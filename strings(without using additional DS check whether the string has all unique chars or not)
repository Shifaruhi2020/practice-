public class Unique {
    static boolean isunique(String str)
    {
        if(str.length()>128)
        {
            return false;
        }
    
        boolean[] check=new boolean[128];
        for(int i=0;i<str.length();i++)
        {
             int val=str.charAt(i);
           if(check[val])
           {
               return false;
           }
           check[val]=true;
        }
        return true;
    }
    public static void main(String args[]) {
      System.out.println(isunique("hello"));
    }
}
